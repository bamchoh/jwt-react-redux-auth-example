  "bugs": {
    "url": "https://github.com/nabeliwo/jwt/issues"
  },
  "homepage": "https://github.com/nabeliwo/jwt#readme",
  "dependencies": {
    "boom": "^3.2.1",
    "config": "^1.20.4",
    "crypt": "0.0.1",
    "handlebars": "^4.0.5",
    "hapi": "^13.4.1",
    "inert": "^4.0.0",
    "jsonwebtoken": "^7.0.0",
    "react": "^15.0.2",
    "react-dom": "^15.0.2",
    "react-redux": "^4.4.5",
    "react-router": "^2.4.1",
    "react-router-redux": "^4.0.4",
    "redux": "^3.5.2",
    "redux-act": "^0.4.2",
